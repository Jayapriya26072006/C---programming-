#include <stdio.h>

int main()
{
    float price;
    float discount;
    float finalPrice;

    printf("Enter price: ");
    scanf("%f", &price);

    printf("Enter discount percentage: ");
    scanf("%f", &discount);

    finalPrice = price - (price * discount / 100);

    printf("Final price = %.2f\n", finalPrice);

    return 0;
}
