#include <stdio.h>

struct emp {
    int id;
    char name[20];
};

int main() {
    struct emp e[3];
    int i, uid;

    for (i = 0; i < 3; i++)
        scanf("%d %s", &e[i].id, e[i].name);

    scanf("%d", &uid);

    for (i = 0; i < 3; i++) {
        if (e[i].id == uid) {
            scanf("%s", e[i].name);
            printf("%d %s", e[i].id, e[i].name);
            return 0;
        }
    }
    return 0;
}
