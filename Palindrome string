#include <stdio.h>
#include <string.h>

int main()
{
    char s[20];
    char r[20];
    int i, j = 0;

    gets(s);

    for (i = strlen(s) - 1; i >= 0; i--)
        r[j++] = s[i];

    r[j] = '\0';

    if (strcmp(s, r) == 0)
        printf("Palindrome\n");
    else
        printf("Not Palindrome\n");

    return 0;
}
