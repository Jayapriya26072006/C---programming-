#include <stdio.h>

int main()
{
    int m1;
    int m2;
    int m3;
    float average;

    printf("Enter mark 1: ");
    scanf("%d", &m1);

    printf("Enter mark 2: ");
    scanf("%d", &m2);

    printf("Enter mark 3: ");
    scanf("%d", &m3);

    average = (m1 + m2 + m3) / 3.0;

    printf("Average marks = %.2f\n", average);

    return 0;
}
