#include <stdio.h>

int main()
{
    char s[50];
    int i = 0;
    int count = 0;

    fgets(s, sizeof(s), stdin);

    while (s[i] != '\0')
    {
        if (s[i]=='a'||s[i]=='e'||s[i]=='i'||s[i]=='o'||s[i]=='u'||
            s[i]=='A'||s[i]=='E'||s[i]=='I'||s[i]=='O'||s[i]=='U')
            count++;

        i++;
    }

    printf("Vowels = %d\n", count);
    return 0;
}
