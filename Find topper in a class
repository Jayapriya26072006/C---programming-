#include <stdio.h>

int main()
{
    int marks[5];
    int i;
    int top;

    for (i = 0; i < 5; i++)
        scanf("%d", &marks[i]);

    top = marks[0];

    for (i = 1; i < 5; i++)
    {
        if (marks[i] > top)
            top = marks[i];
    }

    printf("Top mark = %d\n", top);
    return 0;
}
