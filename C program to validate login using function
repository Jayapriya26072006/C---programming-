#include <stdio.h>
#include <string.h>

int login(char u[], char p[]) {
    return (strcmp(u, "admin") == 0 && strcmp(p, "1234") == 0);
}

int main() {
    char user[20], pass[20];

    printf("Enter username: ");
    scanf("%s", user);
    printf("Enter password: ");
    scanf("%s", pass);

    if (login(user, pass))
        printf("Login Successful\n");
    else
        printf("Invalid Login\n");

    return 0;
}
